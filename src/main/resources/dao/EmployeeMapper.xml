<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jacky.helloworld.dao.EmployeeMapper">

    <select id="getEmpById" resultType="com.jacky.helloworld.bean.Employee">
		select id,last_name lastName,email,gender,age from tbl_employee where id =#{id}
	</select>
</mapper>